memory-map:
  name: reg2
  description: register test 2
  comment: A simple register
  bus: wb-32-be
  x-wbgen:
    hdl_entity: reg2
  children:
  - reg:
      name: R2
      description: Register 2
      comment: Register 2 has two fields
      width: 32
      access: rw
      address: 0x00000000
      children:
      - field:
          name: reset
          description: Reset bit
          comment: 1 to reset
          range: 0
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
      - field:
          name: enable
          description: Enable
          comment: 1 to enable device
          range: 1
          x-wbgen:
            access_bus: READ_WRITE
            access_dev: READ_ONLY
            type: BIT
