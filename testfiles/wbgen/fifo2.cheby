memory-map:
  name: fifo
  description: FIFO 2
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifo2
  children:
  - block:
      name: fifo2
      address: 0x00000000
      size: 8
      description: fifo2
      align: False
      x-wbgen:
        kind: fifo
        direction: CORE_TO_BUS
        depth: 128
        wire_full: True
      children:
      - reg:
          name: fifo2_r0
          description: FIFO 'fifo2' data output register 0
          width: 32
          access: ro
          address: 0x00000000
          children:
          - field:
              name: val
              description: value
              range: 15-0
              x-wbgen:
                type: SLV
      - reg:
          name: fifo2_csr
          description: FIFO 'fifo2' control/status register
          width: 32
          access: ro
          address: 0x00000004
          x-wbgen:
            kind: fifocs
          children:
          - field:
              name: full
              description: FIFO full flag
              comment: |
                1: FIFO 'fifo2' is full
                0: FIFO is not full
              range: 16
              x-wbgen:
                kind: full
                type: BIT
