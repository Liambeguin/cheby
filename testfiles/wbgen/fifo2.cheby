memory-map:
  name: fifo
  description: FIFO 2
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifo2
  schema-version:
    core: 2.0.0
    x-conversions: 1.0.0
    x-driver-edge: 1.0.0
    x-fesa: 2.0.0
    x-gena: 1.0.0
    x-hdl: 1.0.0
    x-map-info: 1.0.0
    x-wbgen: 1.0.0
  children:
  - block:
      name: fifo2
      description: fifo2
      address: 0x0
      align: False
      size: 8
      x-wbgen:
        depth: 128
        direction: CORE_TO_BUS
        kind: fifo
        wire_full: True
      children:
      - reg:
          name: fifo2_r0
          description: 'FIFO ''fifo2'' data output register 0'
          width: 32
          access: ro
          address: 0x0
          children:
          - field:
              name: val
              description: value
              range: 15-0
              x-wbgen:
                type: SLV
      - reg:
          name: fifo2_csr
          description: 'FIFO ''fifo2'' control/status register'
          width: 32
          access: ro
          address: 0x4
          x-wbgen:
            kind: fifocs
          children:
          - field:
              name: full
              description: FIFO full flag
              comment: '1: FIFO ''fifo2'' is full\n0: FIFO is not full'
              range: 16
              x-wbgen:
                kind: full
                type: BIT
