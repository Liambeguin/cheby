memory-map:
  name: pt
  bus: wb-32-be
  description: register constant
  comment: A simple passthrough register
  x-wbgen:
    hdl_entity: rcst1
  children:
  - reg:
      name: R1
      address: 0x00000000
      width: 32
      access: ro
      description: Register 1
      comment: Register 1 has two fields
      children:
      - field:
          name: ver
          range: 0
          description: version
          comment: current version
          preset: 0
          x-wbgen:
            type: CONSTANT
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            size: 1
