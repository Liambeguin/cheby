memory-map:
  name: fifo
  description: FIFO bclr
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifoclr
  children:
  - block:
      name: fifo_bclr
      description: fifo bus clear
      address: 0x0
      align: False
      size: 8
      x-wbgen:
        depth: 256
        direction: BUS_TO_CORE
        kind: fifo
        wire_full: True
      children:
      - reg:
          name: fifo_bclr_r0
          description: 'FIFO ''fifo bus clear'' data input register 0'
          width: 32
          access: wo
          address: 0x0
          children:
          - field:
              name: val
              description: value
              range: 15-0
              x-wbgen:
                type: SLV
      - reg:
          name: fifo_bclr_csr
          description: 'FIFO ''fifo bus clear'' control/status register'
          width: 32
          access: wo
          address: 0x4
          x-wbgen:
            kind: fifocs
          children:
          - field:
              name: clear_bus
              description: FIFO clear
              comment: 'write 1: clears FIFO ''fifo bus clear\nwrite 0: no effect'
              range: 18
              x-wbgen:
                kind: clear_bus
                type: BIT
