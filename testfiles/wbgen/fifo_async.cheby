memory-map:
  name: fifo
  description: FIFO async
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifoasync
  children:
  - block:
      name: fifo1
      address: 0x0
      size: 8
      description: fifo1
      align: False
      x-wbgen:
        kind: fifo
        direction: BUS_TO_CORE
        depth: 256
        clock: clk1
        wire_empty: True
      children:
      - reg:
          name: fifo1_r0
          description: 'FIFO ''fifo1'' data input register 0'
          width: 32
          access: wo
          address: 0x0
          children:
          - field:
              name: val
              description: value
              range: 15-0
              x-wbgen:
                type: SLV
      - reg:
          name: fifo1_csr
          description: 'FIFO ''fifo1'' control/status register'
          width: 32
          access: ro
          address: 0x4
          x-wbgen:
            kind: fifocs
          children:
          - field:
              name: full
              description: FIFO full flag
              comment: '1: FIFO ''fifo1'' is full\n0: FIFO is not full'
              range: 16
              x-wbgen:
                kind: full
                type: BIT
