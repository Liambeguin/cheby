memory-map:
  name: wb_freq_multi_count_csr
  bus: wb-32-be
  description: Frequency Multi Counter registers
  comment: |
    Wishbone slave for Frequency Multi Counters
  x-wbgen:
    hdl_entity: wb_freq_multi_count_csr
  children:
  - memory:
      name: frequency
      address: 0x00000000
      memsize: 128
      description: Frequency RAM
      x-wbgen:
        kind: ram
        access_dev: READ_WRITE
        byte_select: False
      children:
      - reg:
          name: data
          width: 32
          access: rw
