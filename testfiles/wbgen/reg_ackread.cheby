memory-map:
  name: reg1
  description: register ack test
  comment: A simple register
  bus: wb-32-be
  x-wbgen:
    hdl_entity: reg1
  children:
  - reg:
      name: R1
      description: Register 1
      comment: Register 1 has two fields
      width: 32
      access: ro
      address: 0x0
      children:
      - field:
          name: reset
          description: Reset bit
          comment: 1 to reset
          range: 0
          x-wbgen:
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
            ack_read: "true"
            type: BIT
      x-hdl:
        read-strobe: True
