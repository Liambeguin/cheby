memory-map:
  name: reg1
  description: register test 1
  comment: A simple register
  bus: wb-32-be
  x-wbgen:
    hdl_entity: reg1
  children:
  - reg:
      name: R1
      description: Register 1
      comment: Register 1 has two fields
      width: 32
      access: ro
      address: 0x00000000
      children:
      - field:
          name: reset
          range: 0
          description: Reset bit
          comment: 1 to reset
          x-wbgen:
            type: BIT
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
      - field:
          name: enable
          range: 1
          description: Enable
          comment: 1 to enable device
          x-wbgen:
            type: BIT
            access_bus: READ_ONLY
            access_dev: WRITE_ONLY
