memory-map:
  name: fifo
  description: FIFO count
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifocnt
  schema-version:
    core: 2.0.0
    x-conversions: 1.0.0
    x-driver-edge: 1.0.0
    x-fesa: 2.0.0
    x-gena: 1.0.0
    x-hdl: 1.0.0
    x-map-info: 1.0.0
    x-wbgen: 1.0.0
  children:
  - block:
      name: fifo1
      description: fifo1
      address: 0x0
      align: False
      size: 8
      x-wbgen:
        depth: 256
        direction: BUS_TO_CORE
        kind: fifo
        wire_full: True
      children:
      - reg:
          name: fifo1_r0
          description: 'FIFO ''fifo1'' data input register 0'
          width: 32
          access: wo
          address: 0x0
          children:
          - field:
              name: val
              description: value
              range: 15-0
              x-wbgen:
                type: SLV
      - reg:
          name: fifo1_csr
          description: 'FIFO ''fifo1'' control/status register'
          width: 32
          access: ro
          address: 0x4
          x-wbgen:
            kind: fifocs
          children:
          - field:
              name: count
              description: FIFO counter
              comment: 'Number of data records currently being stored in FIFO ''fifo1'''
              range: 7-0
              x-wbgen:
                kind: count
