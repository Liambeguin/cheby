memory-map:
  name: fifo
  description: FIFO count
  bus: wb-32-be
  x-wbgen:
    hdl_entity: fifocnt
  children:
  - block:
      name: fifo1
      address: 0x00000000
      size: 8
      description: fifo1
      align: False
      x-wbgen:
        kind: fifo
        direction: BUS_TO_CORE
        depth: 256
        wire_count: True
      children:
      - reg:
          name: fifo1_r0
          description: FIFO 'fifo1' data input register 0
          width: 32
          access: wo
          address: 0x00000000
          children:
          - field:
              name: val
              description: value
              range: 15-0
              x-wbgen:
                type: SLV
      - reg:
          name: fifo1_csr
          description: FIFO 'fifo1' control/status register
          width: 32
          access: ro
          address: 0x00000004
          x-wbgen:
            kind: fifocs
          children:
          - field:
              name: empty
              description: FIFO empty flag
              comment: |
                1: FIFO 'fifo1' is empty
                0: FIFO is not empty
              range: 17
              x-wbgen:
                kind: empty
                type: BIT
