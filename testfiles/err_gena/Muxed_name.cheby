memory-map:
  name: muxed
  description: Muxed registers
  bus: cern-be-vme-16
  size: 1M
  x-gena:
    map-version: 20180513
  x-map-info:
    ident: 0x00
  schema-version:
    core: 2.0.0
    x-conversions: 1.0.0
    x-driver-edge: 1.0.0
    x-fesa: 2.0.0
    x-gena: 1.0.0
    x-hdl: 1.0.0
    x-map-info: 1.0.0
    x-wbgen: 1.0.0
  children:
  - reg:
      name: muxedRegRO
      description: muxed-reg-ro
      width: 32
      access: ro
      address: next
      x-gena:
        gen:
          mux: regSelX.channelSelect
      x-fesa:
        multiplexed: True
        persistence: True
  - reg:
      name: muxedRegRW
      description: muxed-reg-rw
      width: 32
      access: rw
      address: next
      x-gena:
        gen:
          mux: regSel.channelSelect
      x-fesa:
        multiplexed: True
        persistence: True
  - reg:
      name: regSel
      description: Acquisition buffer selection
      width: 16
      access: rw
      address: next
      x-fesa:
        multiplexed: True
        persistence: True
      children:
      - field:
          name: channelSelect
          description: Channel selection
          range: 15-8
          x-gena:
            gen:
              ext-codes: Mem.memory-channel
      - field:
          name: bufferSelect
          description: Buffer selection
          range: 7-0
  - memory:
      name: Mem
      description: Memory
      address: next
      memsize: 512k
      x-gena:
        memory-channels:
        - memory-channel:
            acq-width: 16
            channel-select-code: 0
            description: chan 0
            name: channel0
        - memory-channel:
            acq-width: 16
            channel-select-code: 1
            description: chan 1
            name: channel1
      x-fesa:
        multiplexed: True
        persistence: True
      children:
      - reg:
          name: Mem
          width: 16
          access: rw
