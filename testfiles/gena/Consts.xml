<memory-map xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" area-depth="256k" description="Test of gen=const" driver-name="RFVXSMAIN" equipment-code="PX-ALGCA" gen="package=my_lib.my_pkg" ident-code="0x00300010" map-version="20190607" mem-map-access-mode="A32/D32" module-type="RF_VXS_MAIN" name="const" note="EDA-02281" semantic-mem-map-version="1.1.0" vme-base-addr="0xb0000000" xsi:schemaLocation="http://abwww.cern.ch/ap/deployments/applications/cern/accsoft/rf/accsoft-rf-cheburashka/PRO/source/src/java/cern/accsoft/rf/cheburashka/xml/schema/TemplateMemMap.xsd">
