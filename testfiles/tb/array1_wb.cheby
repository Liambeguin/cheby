memory-map:
  bus: wb-32-be
  name: array1_wb
  x-hdl:
    busgroup: True
  children:
  - reg:
      name: reg1
      description: A register
      type: unsigned
      width: 32
      access: rw
      preset: 0x12340000
  - reg:
      name: reg2
      type: unsigned
      width: 32
      access: rw
      preset: 0x12340002
  - array:
      name: arr
      repeat: 8
      children:
      - reg:
          name: ram1
          access: rw
          width: 32
  - submap:
      name: sub1_wb
      size: 0x1000
      description: A WB bus
      interface: wb-32-be
  - submap:
      name: sub2_axi4
      size: 0x1000
      description: An AXI4-Lite bus
      interface: axi4-lite-32
